    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-packagejson": "2.4.0",
    "prettier-plugin-sort-json": "1.0.0",
    "tailwindcss": "3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
