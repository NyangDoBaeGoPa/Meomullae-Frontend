    "@types/react-dom": "18.0.10",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "next": "13.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.3",
    "type-check": "0.4.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.4.2",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "autoprefixer": "10.4.13",
    "commitizen": "4.3.0",
    "cz-customizable": "7.0.0",
